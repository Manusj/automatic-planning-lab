(defproblem problem domain
  ;; Initial State
  (
  ;; Type predicates
  (uav u1)
  (uav u2)
  (carrier cr1)
  (carrier cr2)
  (location depot)
  (location l1)
  (location l2)
  (location l3)
  (location l4)
  (location l5)
  (location l6)
  (location l7)
  (location l8)
  (content food)
  (content medicine)
  (crates food 40 depot)
  (crates medicine 40 depot)

  ;; Initial state predicates
  (at u1 depot)
  (at u2 depot)
  (at cr1 depot)
  (at cr2 depot)
  (contains cr1 food 0)
  (contains cr1 medicine 0)
  (contains cr2 food 0)
  (contains cr2 medicine 0)
  (space cr1 4)
  (space cr2 4)
  (heli-free u1)
  (heli-free u2)

  ;; pseudo goals
  (need food 5 l1)
  (need food 1 l2)
  (need medicine 5 l3)
  (need medicine 1 l4)
  (need food 12 l5)
  (need food 8 l6)
  (need medicine 13 l7)
  (need medicine 9 l8)
  )
  ((deliver-all))
)
