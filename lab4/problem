(defproblem problem domain
  ;; Initial State
  (
  ;; Type predicates
  (uav u1)
  (location depot)
  (location l1)
  (content food)
  (content medicine)
  (crate c1)
  (crate c1)
  (person p1)

  ;; Initial state predicates
  (at u1 l1)
  (at p1 l1)
  (heli-free u1)
  (at c1 depot)
  (at c2 depot)
  (crate-content c1 food)
  (crate-content c1 medicine)

  ;; pseudo goals
  (needs p1 food)

  )
  ((deliver-all))
)
